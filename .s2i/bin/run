#!/bin/bash

echo "---> Running conatiner..."

COMMAND=$0

source .venv/bin/activate
source ./envs

case $COMMAND in 
    usage)
        echo "print usage!"
        ;;
    superset-init)
        echo "Running superset initialzations scripts"
        ;;
    superset)
        echo "Running superset with Gunicorn"
        ;;
    *)
        echo "Running bash"
        exec /bin/bash
        ;;
esac
